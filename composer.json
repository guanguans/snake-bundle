{
    "name": "dbu/snake-bundle",
    "type": "bundle",
    "license": "MIT",
    "description": "Snake Game (demo for the Symfony 5.1 console)",
    "authors": [
        {
            "name": "David Buchmann",
            "email": "mail@davidbu.ch"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^5.1"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dbu\\SnakeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbu\\Tests\\SnakeBundle\\Fixtures\\": "tests/fixtures/app/src/",
            "Dbu\\Tests\\SnakeBundle\\": "tests/"
        }
    },
    "scripts": {
    }
}
